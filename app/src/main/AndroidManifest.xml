<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.client.lingkungan_hidup">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LIhatPengaduanActivity"></activity>
        <activity android:name=".ListSatwaAdminActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".DetailLokasiActivity" />
        <activity android:name=".LokasiWisataActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ManipulateSatwaActivity" />
        <activity android:name=".InputPengaduanActivity" />
        <activity android:name=".ListSatwaActivity" />
        <activity android:name=".LoginAdminActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>